{
  "name": "six-cities",
  "version": "1.0.0",
  "private": true,
  "description": "Личный проект «Шесть городов» от HTML Academy",
  "scripts": {
    "eslint": "eslint --ext .jsx --ext .js src/",
    "test": "npm run eslint && npm run test.jest",
    "test.jest": "jest",
    "build": "webpack --mode production",
    "start": "webpack-dev-server"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "5.13.0",
    "eslint-config-htmlacademy": "0.5.2",
    "eslint-plugin-react": "7.20.0",
    "jest": "26.0.1",
    "moment-locales-webpack-plugin": "^1.2.0",
    "react-test-renderer": "16.13.1",
    "redux-mock-store": "^1.5.4",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "leaflet": "1.6.0",
    "moment": "^2.27.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-leaflet": "2.7.0",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "reselect": "^4.0.0",
    "styled-components": "5.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "^App(.*)$": "<rootDir>/src/components/app$1",
      "^CitiesList(.*)$": "<rootDir>/src/components/cities-list$1",
      "^Main(.*)$": "<rootDir>/src/components/main$1",
      "^Empty(.*)$": "<rootDir>/src/components/main-empty$1",
      "^Map(.*)$": "<rootDir>src/components/map$1",
      "^NearbyCard(.*)$": "<rootDir>src/components/nearby-card$1",
      "^NearbyList(.*)$": "<rootDir>src/components/nearby-list$1",
      "^PlaceCard(.*)$": "<rootDir>/src/components/place-card$1",
      "^PlacesList(.*)$": "<rootDir>/src/components/places-list$1",
      "^Property(.*)$": "<rootDir>/src/components/property$1",
      "^ReviewsList(.*)$": "<rootDir>src/components/reviews-list$1",
      "^ReviewItem(.*)$": "<rootDir>src/components/review-item$1",
      "^Sort(.*)$": "<rootDir>src/components/sort$1",
      "^Components(.*)$": "<rootDir>/src/components$1",
      "^Hocs(.*)$": "<rootDir>src/hocs$1",
      "^Utils(.*)$": "<rootDir>/src/utils$1",
      "^Mocks(.*)$": "<rootDir>/src/mocks$1",
      "^Consts(.*)$": "<rootDir>/src/consts$1",
      "^Store(.*)$": "<rootDir>/src/store$1"
    }
  }
}
